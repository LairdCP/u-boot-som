#include "at91-som60_common.dtsi"

/ {
	model = "Ezurio SOM60";
	compatible = "summit,som60", "atmel,sama5d36", "atmel,sama5d3", "atmel,sama5";

	memory@20000000 {
		reg = <0x20000000 0x8000000>;
	};
};

&nand0 {
	ubootspl@0 {
		label = "u-boot-spl";
		reg = <0x0 0x20000>;
	};

	uboot@20000 {
		label = "u-boot";
		reg = <0x20000 0x100000>;
	};

	ubootenv@120000 {
		label = "u-boot-env-a";
		reg = <0x120000 0x20000>;
	};

	ubootenv@140000 {
		label = "u-boot-env-b";
		reg = <0x140000 0x20000>;
	};

	/* Leave last 4 blocks for bbt if nand-on-flash-bbt is defined */
	ubi@160000 {
		label = "ubi";
		reg = <0x160000 0>;
	};
};
