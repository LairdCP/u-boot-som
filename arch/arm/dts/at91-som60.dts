/dts-v1/;
#include "at91-som60.dtsi"

/ {
	model = "Laird DVK SOM60";
	compatible = "laird,dvk-som60", "laird,som60", "atmel,sama5d36", "atmel,sama5d3", "atmel,sama5";

	chosen {
		u-boot,dm-pre-reloc;
		stdout-path = &dbgu;
		tick-timer = &pit;
	};

	signature {
		key-dev {
			/*required = "conf";*/
			algo = "sha256,rsa2048";
			key-name-hint = "dev";
		};
	};
};


&dbgu {
	u-boot,dm-pre-reloc;
	status = "okay";
};

&pit {
	u-boot,dm-pre-reloc;
	status = "okay";
};


&macb0 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	ethernet-phy@7 {
		reg = <7>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_geth_int>;
		interrupt-parent = <&pioB>;
		interrupts = <25 IRQ_TYPE_EDGE_FALLING>;
		txen-skew-ps = <800>;
		txc-skew-ps = <3000>;
		rxdv-skew-ps = <400>;
		rxc-skew-ps = <3000>;
		rxd0-skew-ps = <400>;
		rxd1-skew-ps = <400>;
		rxd2-skew-ps = <400>;
		rxd3-skew-ps = <400>;
	};
};

&macb1 {
	#address-cells = <1>;
	#size-cells = <0>;
	status = "okay";

	ethernet-phy@1 {
		reg = <1>;
		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_eth_int>;
		interrupt-parent = <&pioC>;
		interrupts = <10 IRQ_TYPE_EDGE_FALLING>;
	};
};

&usb0 {
	status = "okay";
};

&usb1 {
	status = "okay";
};

&usb2 {
	status = "okay";
};

