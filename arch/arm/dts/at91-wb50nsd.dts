/dts-v1/;

#include "at91-wb50n.dtsi"

&mmc0 {
	status = "okay";
};

&macb1 {
	status = "okay";

	phy-handle = <&phy1>;

	mdio {
		#address-cells = <1>;
		#size-cells = <0>;

		phy1: ethernet-phy@1 {
			reg = <1>;
		};
	};
};

&usb0 {
	status = "okay";
};
